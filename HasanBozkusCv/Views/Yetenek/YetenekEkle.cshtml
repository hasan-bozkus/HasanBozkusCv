@model HasanBozkusCv.Models.Entity.Skills
@{
    ViewBag.Title = "YetenekEkle";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}


<br />
<h4>Yeni Yetenek Ekleme Sayfası</h4>
<br />
<form class="form-group" method="post">
    @Html.Label("Yetenek")
    @Html.TextBoxFor(x => x.Skill, new { @Class = "form-control" })
    <br />
    @Html.Label("Derece")
    @Html.TextBoxFor(x => x.Ratio, new { @Class = "form-control" })
    <br />
    <button class="btn btn-info">Kaydet</button>
</form>


