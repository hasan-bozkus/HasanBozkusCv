@model HasanBozkusCv.Models.Entity.Certificates 
@{
    ViewBag.Title = "SertifikaEkle";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<br />
<h4> Yeni Sertifika Ekleme Sayfası</h4>
<br />
<form class="form-group" method="post">
    @Html.Label("Açıklama")
    @Html.TextBoxFor(x => x.Descpription, new { @Class = "form-control" })
    <br />
    @Html.Label("Tarih")
    @Html.TextBoxFor(x => x.DateTime, new { @Class = "form-control" })
    <br />
    <button class="btn btn-info">Kaydet</button>
</form>
